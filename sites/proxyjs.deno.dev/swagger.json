{
	"openapi": "3.0.0",
	"info": {
		"title": "Social Media Downloader API",
		"description": "API for downloading content from various social media platforms and managing API configurations",
		"version": "1.0.0",
		"contact": {
			"name": "API Support",
			"url": "https://proxyjs.deno.dev/"
		}
	},
	"servers": [
		{
			"url": "/api",
			"description": "API Server"
		}
	],
	"paths": {
		"/autolink": {
			"post": {
				"summary": "Download content from social media",
				"description": "Download content from various social media platforms including TikTok, Instagram, Facebook, and more",
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"type": "object",
								"required": [
									"url"
								],
								"properties": {
									"url": {
										"type": "string",
										"description": "URL of the content to download"
									},
									"cookie": {
										"type": "string",
										"description": "Optional cookie for private content"
									}
								}
							}
						}
					}
				},
				"responses": {
					"200": {
						"description": "Successful response",
						"content": {
							"application/json": {
								"schema": {
									"type": "object",
									"properties": {
										"url": {
											"type": "string"
										},
										"source": {
											"type": "string"
										},
										"author": {
											"type": "string"
										},
										"title": {
											"type": "string"
										},
										"thumbnail": {
											"type": "string"
										},
										"duration": {
											"type": "string"
										},
										"medias": {
											"type": "array",
											"items": {
												"type": "object",
												"properties": {
													"url": {
														"type": "string"
													},
													"quality": {
														"type": "string"
													},
													"extension": {
														"type": "string"
													},
													"type": {
														"type": "string"
													}
												}
											}
										}
									}
								}
							}
						}
					},
					"400": {
						"description": "Bad Request",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Error"
								}
							}
						}
					}
				}
			}
		}
	},
	"components": {
		"schemas": {
			"Error": {
				"type": "object",
				"properties": {
					"error": {
						"type": "string",
						"description": "Error message"
					}
				}
			}
		}
	}
}