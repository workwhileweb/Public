<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/workwhileweb/Public</PackageProjectUrl>
    <RepositoryUrl>https://github.com/workwhileweb/Public</RepositoryUrl>
    <Description>workwhileweb/Public</Description>
    <Authors>workwhileweb</Authors>
    <ApplicationIcon>favicon.ico</ApplicationIcon>
    <PackageIcon>favicon.png</PackageIcon>
    <PackageIconUrl />
    <PackageId>wWw.Public</PackageId>
    <Version>1.0.2</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="5.0.2" />
  </ItemGroup>

  <ItemGroup>
    <None Include="favicon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
